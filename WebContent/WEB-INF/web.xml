<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1">
  <display-name>Servlet_4_mvc2</display-name>
 	<!-- NoticeController notice = new NoticeController(); Java에서의 객체 생성 : 클래스명, 변수명-->
 	<servlet>
 		<servlet-name>notice</servlet-name>
 		<servlet-class>com.arc.controller.NoticeController</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>notice</servlet-name>
 		<url-pattern>*.notice</url-pattern>
 	</servlet-mapping>
 	
 	<servlet>
 		<servlet-name>member</servlet-name>
 		<servlet-class>com.arc.controller.MemberController</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>member</servlet-name>
 		<url-pattern>/member/memberList.do</url-pattern>
 	</servlet-mapping>
 	
 	
</web-app>